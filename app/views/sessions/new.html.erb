
 <div class="col-sm-8 text-left"> 

     
<h1>Log in</h1>
   
             
<%= form_with(scope: :session, url: sessions_path) do |f| %>

  <%= f.label :email%>
  <%= f.email_field :email ,class:'form-control'%>
<br>
  <%= f.label :password %>
  <%= f.password_field :password ,class:'form-control'%>

  <%= f.submit "Log in" ,class:'btn btn-primary'%>
<% end %></div>
